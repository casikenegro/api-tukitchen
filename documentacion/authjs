/*
  los campos en el body con el signo de exclamación "!" son requeridos
*/
 {
   POST : {
     url: 'api_direccion/auth'
     parametros: {},
     body: {
      rut: string!,
      password: string!
     }
     respuestas: {
       200: {
          "user": {
            rut: "123456",
            dv : 1,
            id_rol: 1,
            id_parent: 1,
          },
          token : "sadasdasd/...asasdasasduasdjasdhasd"
       },
       500: {
         message: 'Error en sus credenciales, intentelo de nuevo por favor',
         message: 'Ha ocurrido un error, contacte con soporte',
       },
     }
   },
   post : {
     url: 'api_direccion/auth_recovery_pass'
     parametros: {},
     body:{
      email: string!,
     }
     respuestas: {
       200: {
        message: "Su nueva contraseña ha sido enviada a su correo"
       },
       422: {
        message: "No se encontro ningún usuario con ese correo",
        message: "El correo suplido tiene un formato incorrecto",
       },
       500: {
         message: "Ha ocurrido un error, contacte con soporte"
       },
     }
   },
}
